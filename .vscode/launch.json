    "version": "0.2.0",
    "configurations": [
        {
            "name": "TestWebApp Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/test/DNTFrameworkCore.TestWebApp/bin/Debug/netcoreapp2.2/DNTFrameworkCore.TestWebApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/test/DNTFrameworkCore.TestWebApp",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "TestAPI Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/test/DNTFrameworkCore.TestAPI/bin/Debug/netcoreapp2.2/DNTFrameworkCore.TestAPI.dll",
            "args": [],
            "cwd": "${workspaceFolder}/test/DNTFrameworkCore.TestAPI",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}